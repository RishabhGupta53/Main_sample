function Add-Entries {
    param (
        [string]$keyPrefix,
        [string]$csvValues,
        [hashtable]$backendPools
    )
    $i = 1
    foreach ($val in $csvValues.Split(',') | ForEach-Object { $_.Trim() }) {
        Write-Host "$val"
        if ($val) {
            $key = "{0}_{1}_{2:D3}" -f $backend_pool, $keyPrefix, $i
            $backendPools[$key] = @($val)
            $i++
        }
    }
}
